{
  "name": "GIST",
  "private": false,
  "version": "0.2.1",
  "main": "dist-electron/main/index.js",
  "type": "commonjs",
  "homepage": "https://carsonthemonkey.github.io/GIST/",
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "scripts": {
    "start": "electron .",
    "dev": "vite",
    "dev-electron" : "vite --config vite.config.electron.ts",
    "build": "tsc && vite build",
    "build-electron" : "tsc && vite build --config vite.config.electron.ts && electron-builder",
    "preview": "vite preview",
    "predeploy": "node build_tools/confirmDeploy.js && npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "electron-updater": "^5.3.0",
    "openai": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-latex": "^2.0.0",
    "react-markdown": "^8.0.7",
    "react-mathjax-ts": "^0.1.2",
    "react-modal": "^3.16.1",
    "rehype-katex": "^6.0.2",
    "remark-math": "^5.1.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.32.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-latex": "^2.0.0",
    "@types/react-modal": "^3.13.1",
    "@vitejs/plugin-react": "^3.1.0",
    "electron": "^23.2.0",
    "electron-builder": "^24.2.1",
    "gh-pages": "^5.0.0",
    "readline-sync": "^1.4.10",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-electron": "^0.11.2",
    "vite-plugin-electron-renderer": "^0.14.1"
  }
}
